<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.dalilu.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_alerts"
        android:name="com.dalilu.ui.notification.NotificationsFragment"
        android:label="@string/title_alerts"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.dalilu.ui.contacts.ContactsFragment"
        android:label="@string/frnds"
        tools:layout="@layout/fragment_contacts" />
</navigation>